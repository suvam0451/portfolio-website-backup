{
	"defaultSeverity": "error",
	"extends": [
		"tslint:latest",
		"tslint-react",
		"tslint-config-prettier"
	],
	"jsRules": {},
	"rules": {
		"interface-name": false,
		"semicolon": [false, "never"],
		"member-access": [false],
		"ordered-imports": [false],
		"no-console": [false],
		"no-var-requires": true,
		"quotemark": [true, "double", "jsx-double"]
	},
	"rulesDirectory": []
}
